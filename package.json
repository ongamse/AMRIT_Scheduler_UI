  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "postinstall": "node version.js && chmod +x ./scripts/*.js",
    "start": "node version.js && ng serve --port 4300",
    "build": "ng build",
    "test": "ng test",
    "report": "karma start --reporters kjhtml",
    "coverage": "ng test --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-dev": "node --max_old_space_size=5048 ./node_modules/@angular/cli/bin/ng build --prod --aot",
    "build-test": "node --max_old_space_size=5048 ./node_modules/@angular/cli/bin/ng build -e test --aot",
    "build-ci": "./scripts/ci-prebuild.js && node --max_old_space_size=5048 ./node_modules/@angular/cli/bin/ng build --prod --environment=ci --aot"
  },
  "private": true,
